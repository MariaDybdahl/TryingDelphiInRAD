function GetHTTP: string;
  begin
     FullURL := ServerAddress + '?results=10';
     http:= TIdHTTP.Create(nil);
     try


     Result := http.Get(FullURL);


     finally
       http.Free;
     end;

  end;

  function CleanJsonFormToCleanList(Const Users: string): TStringList
    begin

         JSONStr := GetHTTP;


    end;
end.

