unit uMainForm;

interface

uses
  Winapi.Windows, Winapi.Messages, System.SysUtils, System.Variants, System.Classes, Vcl.Graphics,
  Vcl.Controls, Vcl.Forms, Vcl.Dialogs, Vcl.StdCtrls, IdHTTP, System.JSON;

type
  TForm1 = class(TForm)
  btnText: TButton;
  textFromBTN: TStaticText;
  ListBox1: TListBox;
  procedure btnTextClick(Sender: TObject);
  private
    { Private declarations }
  public
    { Public declarations }
  end;

var
  http: TIdHTTP;
  Form1: TForm1;
  jsonStr: string;
  jsonArr: TJSONArray;
  jsonObj: TJSONObject;
  i: Integer;

implementation

procedure TForm1.btnTextClick(Sender: TObject);


begin
textFromBTN.Caption := 'Henter 10 users';
http := TIdHTTP.Create(nil);
  try
    jsonStr := http.Get('https://jsonplaceholder.typicode.com/users');
    jsonArr := TJSONObject.ParseJSONValue(jsonStr) as TJSONArray;

  
  finally
    http.Free;
  end;

end;

end.

